{
  "name": "vue-vant-template",
  "version": "2.1.0",
  "description": "A vue mobile template with Vue-CLI, Vuex, Vue-Router, Vant, Axios, Scss, Mock, Eslint and Viewport adaptation scheme...",
  "author": "Jackie <JackieCheung517@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "new": "plop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "vant",
    "boilerplate",
    "vue-vant-template",
    "vue-mobile-template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JackieCheung/vue-vant-template.git"
  },
  "bugs": {
    "url": "https://github.com/JackieCheung/vue-vant-template/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "async-validator": "^3.5.1",
    "axios": "^0.21.1",
    "core-js": "^3.8.1",
    "dayjs": "^1.10.2",
    "driver.js": "^0.9.8",
    "dropzone": "^5.7.2",
    "express": "^4.17.1",
    "fastclick": "^1.0.6",
    "file-saver": "^2.0.5",
    "fuse.js": "^6.4.6",
    "js-base64": "^3.6.0",
    "js-cookie": "^2.2.1",
    "jsonlint": "^1.6.3",
    "jszip": "^3.5.0",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.0",
    "quill": "^1.3.7",
    "script-loader": "^0.7.2",
    "sortablejs": "^1.10.2",
    "vant": "^2.12.3",
    "vue": "^2.6.12",
    "vue-cropper": "^0.5.6",
    "vue-pdf": "^4.2.0",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-eslint": "^4.5.10",
    "@vue/cli-plugin-unit-jest": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/test-utils": "^1.1.2",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-lodash": "^3.3.4",
    "chalk": "^4.1.0",
    "chokidar": "^3.5.0",
    "compression-webpack-plugin": "^6.1.1",
    "connect": "^3.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-plugin-vue": "^7.4.1",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "lodash-webpack-plugin": "^0.11.6",
    "mockjs": "^1.1.0",
    "multer": "^1.4.2",
    "plop": "^2.7.4",
    "postcss-px-to-viewport": "^1.1.1",
    "sass": "^1.32.0",
    "sass-loader": "^10.1.0",
    "scp2": "^0.5.0",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "script-loader": "^0.7.2",
    "serve-static": "^1.14.1",
    "ssh2": "^0.8.9",
    "svg-sprite-loader": "^5.2.1",
    "svgo": "^1.3.2",
    "tasksfile": "^5.1.1",
    "vue-template-compiler": "^2.6.12"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "Android >= 4.0",
    "iOS >= 8"
  ]
}
